{
  "name": "website",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "_generate-supported-browsers": "browserslist --json > src/supportedBrowsers.json",
    "_generate-supported-browsers-regexp": "echo \"const supportedBrowsersRegExp = $(browserslist-useragent-regexp --allowHigherVersions);\n\nexport { supportedBrowsersRegExp };\" > src/supportedBrowsersRegExp.ts",
    "build": "rimraf docs && webpack --config webpack.prod.js",
    "build-browser-check": "yarn _generate-supported-browsers && yarn _generate-supported-browsers-regexp && webpack --config webpack.browserCheck.js",
    "dev": "webpack-dev-server --config webpack.dev.js",
    "start": "http-server docs",
    "test": "tsc --noEmit"
  },
  "browserslist": [
    "extends browserslist-config-google/no-ie",
    "not firefox >= 81"
  ],
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.15.0",
    "@material-ui/core": "^4.12.2",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.60",
    "@proofdict/textlint-rule-proofdict": "^3.1.2",
    "@sentry/browser": "^5.13.0",
    "@textlint-ja/textlint-rule-no-dropping-i": "hata6502/textlint-rule-no-dropping-i",
    "@textlint-ja/textlint-rule-no-filler": "^1.1.0",
    "@textlint-ja/textlint-rule-no-insert-dropping-sa": "hata6502/textlint-rule-no-insert-dropping-sa",
    "@textlint-ja/textlint-rule-no-synonyms": "^1.3.0",
    "@textlint/kernel": "^3.2.1",
    "@textlint/textlint-plugin-text": "^4.1.13",
    "@types/react": "^17.0.14",
    "@types/react-dom": "^17.0.9",
    "@types/react-helmet": "^6.1.2",
    "@types/styled-components": "^5.0.1",
    "@types/uuid": "^7.0.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-istanbul": "^6.0.0",
    "babel-plugin-static-fs": "^3.0.0",
    "browserslist": "^4.16.1",
    "browserslist-config-google": "^2.0.0",
    "browserslist-useragent-regexp": "^2.1.1",
    "copy-webpack-plugin": "^5.1.1",
    "core-js": "3",
    "http-server": "^0.12.1",
    "path": "^0.12.7",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.0.0",
    "react-is": "^17.0.2",
    "regenerator-runtime": "^0.13.5",
    "rimraf": "^3.0.2",
    "styled-components": "^5.0.1",
    "sudachi-synonyms-dictionary": "^5.0.1",
    "textlint-filter-rule-ja-named-entities": "^1.0.0",
    "textlint-filter-rule-urls": "^1.0.1",
    "textlint-rule-date-weekday-mismatch": "^1.0.5",
    "textlint-rule-en-spell": "^1.2.2",
    "textlint-rule-general-novel-style-ja": "^1.3.0",
    "textlint-rule-ja-hiragana-daimeishi": "hata6502/textlint-rule-ja-hiragana-daimeishi",
    "textlint-rule-ja-hiragana-fukushi": "hata6502/textlint-rule-ja-hiragana-fukushi",
